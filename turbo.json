{
	"$schema": "https://turborepo.org/schema.json",
	"pipeline": {
		"@codeui/playground#build": {
			"dependsOn": [
				"@codeui/kit#build"
			]
		},
		"build": {
			"dependsOn": [
				"^build"
			],
			"outputs": [
				"dist/**"
			]
		},
		"test": {
			"dependsOn": [
				"^build"
			]
		},
		"typecheck": {
			"dependsOn": [
				"^build"
			]
		},
		"dev": {
			"cache": false
		}
	}
}
